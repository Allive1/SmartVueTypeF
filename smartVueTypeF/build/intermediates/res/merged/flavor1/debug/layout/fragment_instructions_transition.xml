<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/LinearLayout2"
    android:layout_width="match_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical" >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="759dp">

        <FrameLayout
            android:id="@+id/caliFrame"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true">

            <include
                layout="@layout/ocv_content"
                android:id="@+id/include"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

        </FrameLayout>

        <FrameLayout
            android:id="@+id/scene_root"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <include
                android:id="@+id/phase_frag"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                layout="@layout/phase_page"
                android:textAlignment="center" />

        </FrameLayout>

    </FrameLayout>

    <Button
        android:id="@+id/next_button"
        android:layout_width="161dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="108dp"
        android:layout_marginStart="110dp"
        android:text="@string/next"
        android:layout_gravity="center_horizontal|bottom" />


</LinearLayout><!-- From: file:/Users/Omi/AndroidStudioProjects/SmartVueTypeF/smartVueTypeF/src/main/res/layout/fragment_instructions_transition.xml -->